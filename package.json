{
  "name": "@s4p/music-api",
  "version": "1.8.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "gulp lint",
    "lint-fix": "./node_modules/.bin/eslint src/**/*.ts --fix",
    "watch": "gulp",
    "build": "gulp build",
    "test": "TS_NODE_FILES=true ava --fail-fast -v",
    "coverage": "TS_NODE_FILES=true nyc ava --fail-fast -v"
  },
  "dependencies": {
    "NeteaseCloudMusicApi": "Binaryify/NeteaseCloudMusicApi",
    "lodash": "^4.17.11",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@s4p/eslint-config": "^2.0.1",
    "@types/lodash": "^4.14.117",
    "@types/node": "^10.11.7",
    "@types/request-promise": "^4.1.42",
    "@typescript-eslint/eslint-plugin": "^1.2.0",
    "ava": "^1.2.1",
    "del": "^3.0.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "gulp": "4.0.0",
    "gulp-cached": "^1.1.1",
    "gulp-eslint": "^5.0.0",
    "gulp-filter": "^5.1.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-remember": "^1.0.1",
    "gulp-replace": "^1.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^5.0.0-alpha.3",
    "node-notifier": "^5.2.1",
    "nodemon": "^1.17.4",
    "nyc": "^13.2.0",
    "source-map-support": "^0.5.10",
    "streamqueue": "^1.1.2",
    "ts-node": "^8.0.2",
    "typescript": "~3.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
